                   <Label x:Name = "friDbCurls1Label"  Grid.Row = "20" Grid.Column = "0" Text = "x10" HorizontalOptions = "End" VerticalOptions = "Center" />
                   <Switch x:Name = "friDbCurls1Check" Grid.Row =  "20" Grid.Column = "2" HorizontalOptions = "Center" VerticalOptions = "Center"/>
                    
