                    <ColumnDefinition Width = "2.5*" />
                </Grid.ColumnDefinitions>
              <Button x:Name = "friBenchButton" HorizontalOptions = "End" VerticalOptions = "Center" Text = "Bench" Grid.Row = "0" Grid.Column = "1" HeightRequest = "75" WidthRequest = "200" BackgroundColor = "Black" TextColor = "White"/>
